{
  "name": "@dailydotdev/node-common",
  "version": "0.0.2",
  "description": "Shared packages for daily.dev Node.js projects",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dailydotdev/node-common.git"
  },
  "contributors": [
    "omBratteng"
  ],
  "type": "module",
  "exports": {
    "./*": {
      "typescript": "./src/*/*.ts",
      "import": {
        "types": "./dist/*/*.d.ts",
        "default": "./dist/*/*.js"
      },
      "require": {
        "types": "./dist/*/*.d.cts",
        "default": "./dist/*/*.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@google-cloud/pino-logging-gcp-config": "^1.3.1",
    "@opentelemetry/api": "^1.9.0",
    "pino": "^10.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "22.x",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "packageManager": "pnpm@10.29.3"
}
